{
	"name": "@aetherjs/eslint-config",
	"description": "Standard ESLint config for AetherJS",
	"author": "@aetherjs",
	"version": "0.1.4",
	"license": "MIT",
	"sideEffects": false,
	"main": "dist/index.js",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"scripts": {
		"build": "tsup",
		"test": "vitest run",
		"prepack": "yarn build",
		"release": "cliff-jumper",
		"changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/eslint-config/*'"
	},
	"homepage": "https://github.com/aether-development/utilities/tree/main/packages/eslint-config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aether-development/utilities.git",
		"directory": "packages/eslint-config"
	},
	"keywords": [
		"aether-development",
		"aetherjs",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"standalone"
	],
	"bugs": {
		"url": "https://github.com/aether-development/utilities/issues"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"prettier": "^3.0.0",
		"typescript": "^5.0.0"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^1.8.8",
		"tsup": "^7.0.0"
	},
	"engines": {
		"node": ">=v16.0.0",
		"npm": ">=8.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
