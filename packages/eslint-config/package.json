{
	"name": "@aetherjs/eslint-config",
	"description": "Standard ESLint config for AetherJS",
	"author": "@aetherjs",
	"version": "0.0.2",
	"license": "MIT",
	"sideEffects": false,
	"main": "dist/index.js",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"scripts": {
		"build": "tsup",
		"test": "vitest run",
		"prepack": "yarn build",
		"release": "cliff-jumper",
		"changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/eslint-config/*'"
	},
	"homepage": "https://github.com/aether-development/utilities/tree/main/packages/config-eslint",
	"repository": {
		"type": "git",
		"url": "https://github.con/aether-development/utilities.git",
		"directory": "packages/config-eslint"
	},
	"keywords": [
		"aether-development",
		"aetherjs",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"standalone"
	],
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.7.1",
		"typescript": "^4.7.4"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^1.8.5",
		"tsup": "^6.1.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
