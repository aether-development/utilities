{
	"name": "@aetherjs/prettier-config",
	"description": "Standard Prettier config for AetherJS",
	"author": "@aetherjs",
	"version": "0.0.3",
	"license": "MIT",
	"sideEffects": false,
	"main": "dist/index.js",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"scripts": {
		"build": "tsup",
		"test": "vitest run",
		"prepack": "yarn build",
		"release": "cliff-jumper",
		"lint": "eslint src tests --ext ts --fix",
		"changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/prettier-config/*'"
	},
	"repository": {
		"type": "git",
		"url": "https://github.con/aether-development/utilities.git",
		"directory": "packages/prettier-config"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"dependencies": {
		"prettier": "^2.7.1"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^1.8.5",
		"tsup": "^6.1.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
