{
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "build/**"]
		},
		"lint": {
			"dependsOn": ["@aetherjs/config-prettier#build", "@aetherjs/config-eslint#build"],
			"outputs": []
		},
		"test": {
			"dependsOn": [],
			"outputs": ["coverage/**"]
		},
		"bump": {
			"dependsOn": [],
			"outputs": ["CHANGELOG.md"]
		},
		"check-update": {
			"dependsOn": [],
			"outputs": []
		},
		"cl": {
			"dependsOn": [],
			"outputs": []
		}
	}
}
